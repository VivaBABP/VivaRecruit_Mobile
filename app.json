{
  "expo": {
    "name": "VivaRecruts_Mobile",
    "slug": "VivaRecruts_Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/logo_vivatech_icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ]
    ],
    "splash": {
      "image": "./assets/logo/splash_vivatech.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/adaptive-icon_vivatech.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/logo/logo_vivatech.png"
    }
  }
}
